<Window x:Class="InterestRateModellingTool.Series.Copom.Meetings.CopomMeetingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterestRateModellingTool.Series.Copom.Meetings"
        mc:Ignorable="d"
        Title="CopomMeeting" Height="300" Width="300">

    <Window.DataContext>
        <local:Model/>
    </Window.DataContext>

    <Grid>
        <DataGrid ItemsSource="{Binding Path=CopomMeetings, Mode=OneWay}" AutoGenerateColumns="True" SelectionUnit="Cell">
        </DataGrid>
    </Grid>
</Window>
